package com.soft863.entity;

import java.io.Serializable;

/**
 * 角色与角色权限中间表
 * @author Administrator
 *
 */
public class Role_Code implements Serializable {

	private static final long serialVersionUID = 9206860851948055870L;
	private Integer role_id;
	private Integer code_id;
	
	public Role_Code() {
		// TODO Auto-generated constructor stub
	}
	public Role_Code(Integer role_id) {
		super();
		this.role_id = role_id;
	}

	public Role_Code(Integer role_id, Integer code_id) {
		super();
		this.role_id = role_id;
		this.code_id = code_id;
	}

	public Integer getRole_id() {
		return role_id;
	}
	
	public void setRole_id(Integer role_id) {
		this.role_id = role_id;
	}

	public Integer getCode_id() {
		return code_id;
	}
	
	public void setCode_id(Integer code_id) {
		this.code_id = code_id;
	}

	@Override
	public String toString() {
		return "role_code [role_id=" + role_id + ", code_id=" + code_id + "]";
	}
	
	
}
